---
import Product from './Product.astro'
import { getProductList } from '../services/products.ts'

const { products } = await getProductList()
---

{
	products &&
		products.map(
			({ id, title: productName, price: productPrice, images: productImage }) => (
				<Product
					{...{
						id,
						productName,
						productPrice,
						productImage,
					}}
				/>
			),
		)
}
