---
import Product from './Product.astro'
import { getProductList } from '../../../commons/services/products.ts'

const { products } = await getProductList()
---

{
	products &&
		products.map(({ id, title, description, price, images }) => (
			<Product
				{...{
					id,
					title,
					description,
					price,
					images,
				}}
			/>
		))
}
